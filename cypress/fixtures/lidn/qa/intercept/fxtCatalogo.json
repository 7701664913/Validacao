{
    "name": "vv-jornada-catalogo-ngx",
    "version": "5.23.0-15",
    "license": "MIT",
    "publishConfig": {
      "registry": "http://nexus.viavarejo.com.br/repository/npm-private/"
    },
    "scripts": {
      "ng": "ng",
      "dev": "ng serve casas-bahia --proxy-config proxy.conf.json --vendor-source-map",
      "dev:local": "ng serve casas-bahia --host 0.0.0.0 --proxy-config proxy.conf.json --vendor-source-map",
      "dev-aot": "ng serve casas-bahia --proxy-config proxy.conf.json --optimization --aot",
      "dev-pf": "ng serve pontofrio --proxy-config proxy-pf.conf.json",
      "dev-pf:local": "ng serve pontofrio --host 0.0.0.0 --proxy-config proxy-pf.conf.json",
      "dev-pf-aot": "ng serve pontofrio --optimization --aot",
      "build:cb": "ng build casas-bahia --configuration=production --optimization --aot --prod && npm run estilos:cb",
      "build:pf": "ng build pontofrio --configuration=production --optimization --aot --prod && npm run estilos:pf",
      "test": "ng test",
      "test:ci": "ng test casas-bahia --browsers=Headless_Firefox --code-coverage=true --watch=false",
      "test:xvfb": "xvfb-run npm run test:ci ",
      "lint": "ng lint",
      "lint:sass": "sass-lint -c ./.sass-lint.yml -v -q",
      "coverage": "ng test --watch=false --code-coverage",
      "sonar": "node .\\sonar-project.js",
      "e2e": "ng e2e",
      "e2e:xvfb": "export http_proxy=http://10.128.131.16:3128 && export https_proxy=http://10.128.131.16:3128 && export no_proxy='127.0.0.1, localhost' && xvfb-run npm run e2e",
      "doc:build": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
      "doc:serve": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s",
      "estilos:cb": "sh ./scripts/estilos-casas-bahia.sh",
      "estilos:pf": "sh ./scripts/estilos-pontofrio.sh",
      "update-libs-css": "sh ./scripts/estilos-libs-local.sh"
    },
    "dependencies": {
