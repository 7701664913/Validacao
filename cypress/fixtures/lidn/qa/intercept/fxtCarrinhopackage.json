{
    "name": "vv-jornada-carrinho-ngx",
    "version": "4.41.0-3",
    "license": "MIT",
    "publishConfig": {
      "registry": "http://nexus.viavarejo.com.br/repository/npm-private/"
    },
    "scripts": {
      "ng": "ng",
      "dev": "ng serve casas-bahia --proxy-config proxy.conf.json --vendor-source-map",
      "dev-aot": "ng serve casas-bahia --proxy-config proxy.conf.json --optimization --aot --prod",
      "dev-pf": "ng serve pontofrio --proxy-config proxy-pf.conf.json",
      "dev-pf-aot": "ng serve pontofrio --optimization --aot --prod",
      "build:cb": "ng build casas-bahia --configuration=production --optimization --aot --prod && cp package.json dist-casas-bahia/package.json",
      "build:pf": "ng build pontofrio --configuration=production --optimization --aot --prod && cp package.json dist-pontofrio/package.json",
      "build:ci": "npm run build:cb && npm run build:pf",
      "setversion": "./setversion.sh",
      "test": "ng test",
      "test-coverage": "ng test --watch=false --code-coverage",
      "test:ci": "ng test casas-bahia --browsers=Headless_Firefox --code-coverage=true --watch=false",
      "test:xvfb": "xvfb-run npm run test:ci ",
      "lint": "ng lint",
      "lint:sass": "sass-lint -c ./.sass-lint.yml -v -q",
      "e2e": "ng e2e",
      "e2e:xvfb": "export http_proxy=http://10.128.131.16:3128 && export https_proxy=http://10.128.131.16:3128 && export no_proxy='127.0.0.1, localhost' && xvfb-run npm run e2e",
      "doc:build": "./node_modules/.bin/compodoc -p src/tsconfig.app.json",
      "doc:serve": "./node_modules/.bin/compodoc -p src/tsconfig.app.json -s",
      "sonar-scanner": "node ./sonar-project.js",
      "build-docker:cb": "ng build casas-bahia --aot --configuration=production",
      "build-docker:pf": "ng build pontofrio --aot --configuration=production",
      "build:stats": "ng build --stats-json",
      "analyze": "webpack-bundle-analyzer dist-casas-bahia/stats.json"
    },
    "husky": {
      "hooks": {
        "pre-push": "npm run lint && npm run lint:sass"
      }
    },
